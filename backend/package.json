{
  "name": "amount-detect",
  "version": "1.0.0",
  "description": "Service for extracting and classifying monetary amounts from medical bills/receipts",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm install && npm run build:prod",
    "build:prod": "echo \"Production build complete\"",
    "docs:generate": "node -e \"console.log('Documentation will be available at http://localhost:3000/api-docs when the server is running')\"",
    "docs:serve": "node src/index.js",
    "lint": "eslint . --ext .js",
    "test": "jest"
  },
  "keywords": [
    "ocr",
    "medical-bills",
    "amount-extraction"
  ],
  "author": "AJ Harsh Vardhan",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "multer": "^2.0.2",
    "sharp": "^0.32.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^4.1.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
