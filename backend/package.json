{
  "name": "amount-detect",
  "version": "1.0.0",
  "description": "Service for extracting and classifying monetary amounts from medical bills/receipts",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:acceptance": "jest tests/acceptance --coverage",
    "test:api": "jest tests/api.test.js --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:acceptance",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:update": "jest --updateSnapshot"
  },
  "keywords": [
    "ocr",
    "medical-bills",
    "amount-extraction"
  ],
  "author": "AJ Harsh Vardhan",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "sharp": "^0.32.0",
    "tesseract.js": "^4.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
